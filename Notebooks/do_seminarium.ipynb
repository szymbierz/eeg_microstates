    "x = np.arange(0,101)\n",
    "def oslabianie(I_0,mu,x):\n",
    "    return I_0*np.exp(-mu*x)\n",
    "I = oslabianie(1000,0.15,x)\n",
    "\n",
    "plt.plot(x,I)\n",
    "plt.xlabel(\"Grubość tkanki [cm]\")\n",
    "plt.ylabel(\"Natężenie promieniowania I(x)\")"
